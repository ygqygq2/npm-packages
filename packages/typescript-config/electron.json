{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Electron",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "ESNext"
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.d.ts"]
}
